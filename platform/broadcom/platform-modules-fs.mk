# FS Platform modules

FS_N8550_48B8C_PLATFORM_MODULE_VERSION = 1.1
FS_N8550_32C_PLATFORM_MODULE_VERSION = 1.1


export FS_N8550_48B8C_PLATFORM_MODULE_VERSION
export FS_N8550_32C_PLATFORM_MODULE_VERSION



FS_N8550_48B8C_PLATFORM_MODULE = sonic-platform-fs-n8550-48b8c_$(FS_N8550_48B8C_PLATFORM_MODULE_VERSION)_amd64.deb
$(FS_N8550_48B8C_PLATFORM_MODULE)_SRC_PATH = $(PLATFORM_PATH)/sonic-platform-modules-fs
$(FS_N8550_48B8C_PLATFORM_MODULE)_DEPENDS += $(LINUX_HEADERS) $(LINUX_HEADERS_COMMON) $(PDDF_PLATFORM_MODULE_SYM)
$(FS_N8550_48B8C_PLATFORM_MODULE)_PLATFORM = x86_64-fs_n8550_48b8c
SONIC_DPKG_DEBS += $(FS_N8550_48B8C_PLATFORM_MODULE)


FS_N8550_32C_PLATFORM_MODULE = sonic-platform-fs-n8550-32c_$(FS_N8550_32C_PLATFORM_MODULE_VERSION)_amd64.deb
$(FS_N8550_32C_PLATFORM_MODULE)_PLATFORM = x86_64-fs_n8550_32c
$(eval $(call add_extra_package,$(FS_N8550_48B8C_PLATFORM_MODULE),$(FS_N8550_32C_PLATFORM_MODULE)))

